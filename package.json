{
  "name": "react-typescript-boilerplate",
  "version": "0.0.1",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "single page application",
    "boilerplate",
    "react",
    "typescript"
  ],
  "scripts": {
    "fix:prettier": "prettier \"{,!(dist|temp|bundle)/**/}*.{ts,tsx}\" --write",
    "build": "rimraf bundle && webpack --config webpack.prod.js",
    "start": "webpack-dev-server --progress",
    "test": "jest --coverage",
    "lint": "eslint . --ext .tsx,.ts --cache"
  },
  "private": true,
  "publishConfig": {
    "access": "private"
  },
  "author": "Sense/Net <snteam@sensenet.com> (http://sensenet.com/)",
  "license": "GPL-2.0",
  "homepage": "https://sensenet.com",
  "devDependencies": {
    "@types/enzyme": "^3.9.3",
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^3.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsdoc": "^15.8.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^2.0.1",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.4",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.3",
    "prettier": "^1.17.1",
    "rimraf": "^3.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "ts-config-webpack-plugin": "^1.3.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2",
    "url-loader": "^2.0.1",
    "webpack": "^4.32.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@furystack/inject": "4.0.2",
    "@furystack/logging": "^2.0.3",
    "@material-ui/core": "^4.2.0",
    "@sensenet/client-utils": "^1.6.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "tslib": "^1.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ext .tsx,.ts --cache --fix",
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/test",
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/jest/cssTransform.js"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.css$": "<rootDir>/jest/cssTransform.js",
      "^.+\\.(png|jpg|gif)$": "<rootDir>/jest/fileTransform.js"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig-base.json"
      }
    },
    "testMatch": [
      "**/test/*.+(ts|tsx|js)",
      "**/test/**/*.+(ts|tsx|js)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageReporters": [
      "json",
      "html"
    ]
  }
}
